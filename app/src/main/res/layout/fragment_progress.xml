<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.ProgressFragment">

   <TextView
       android:id="@+id/dulica_denisa_progress_tv"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="@string/progress_tv"
       app:layout_constraintTop_toTopOf="parent"
       android:textSize="32dp"
       android:layout_marginTop="64dp"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toStartOf="parent"
      >
   </TextView>

   <ProgressBar
       android:id="@+id/dulica_denisa_progress_bar"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       style="@style/Widget.AppCompat.ProgressBar.Horizontal"
       android:max="10"
       android:layout_marginStart="8dp"
       android:layout_marginEnd="8dp"
       app:layout_constraintEnd_toEndOf="parent"
       app:layout_constraintStart_toStartOf="parent"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintBottom_toBottomOf="parent">

   </ProgressBar>

    <TextView
        android:id="@+id/dulica_denisa_fragment_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="334dp"
        android:text="1"
        android:textSize="32dp"
        android:layout_marginStart="16dp"
        app:layout_constraintEnd_toStartOf="@+id/dulica_denisa_fragment_tv_b"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dulica_denisa_progress_bar"
        >

    </TextView>

   <TextView
       android:id="@+id/dulica_denisa_fragment_tv_b"
       android:layout_width="0dp"
       android:layout_height="wrap_content"
       android:text="@string/bara"
       android:textSize="32dp"
       app:layout_constraintTop_toBottomOf="@+id/dulica_denisa_progress_bar"
       app:layout_constraintStart_toEndOf="@+id/dulica_denisa_fragment_tv"
       app:layout_constraintEnd_toEndOf="parent"

       >

   </TextView>

    <TextView
        android:id="@+id/dulica_denisa_fragment_tv_10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="10"
        android:layout_marginStart="12dp"
        android:textSize="32dp"
        app:layout_constraintTop_toBottomOf="@+id/dulica_denisa_progress_bar"
        app:layout_constraintStart_toEndOf="@+id/dulica_denisa_fragment_tv"
        app:layout_constraintEnd_toEndOf="parent"

        >

    </TextView>


</androidx.constraintlayout.widget.ConstraintLayout>